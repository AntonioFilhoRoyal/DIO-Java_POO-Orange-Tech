package edu.dio.bootcamp;

import java.time.LocalDate;

public class Mentoria extends Conteudo{

	private LocalDate data;
	
	@Override
	public double calcularXp() {
		return data + 10;
	}

	public LocalDate getData() {
		return data;
	}

	public void setData(LocalDate data) {
		this.data = data;
	}

}
